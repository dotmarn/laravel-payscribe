{
    "name": "dotmarn/laravel-payscribe",
    "description": "A laravel package for payscribe",
    "keywords": [
        "php",
        "laravel",
        "payscribe",
        "payments",
        "fintech",
        "laravel 8",
        "laravel 9",
        "laravel 10"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ridwan Kasim",
            "email": "kasimsheyi20@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "illuminate/support": "~7|~8|~9|^10.0",
        "guzzlehttp/guzzle": "~7|~8|~9"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4|^9.0",
        "scrutinizer/ocular": "~1.1",
        "php-coveralls/php-coveralls": "^2.0",
        "mockery/mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Dotmarn\\Payscribe\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dotmarn\\Payscribe\\PayscribeServiceProvider"
            ]
        }
    }
}